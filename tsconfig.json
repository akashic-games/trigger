{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2021",
		"lib": ["ES2021"],
		"moduleResolution": "Node16",
		"declaration": true,
		"noImplicitAny": true,
		"outDir": "./lib",
		"sourceMap": false,
		"strict": true
	},
	"include": ["src/**/*.ts"],
	"exclude": ["src/__tests__/**/*.ts"]
}
